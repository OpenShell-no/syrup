# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = tab
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 9999

[*.nsi.j2]
indent_style = tab
indent_size = 4

[*.py]
indent_style = space
indent_size = 4
